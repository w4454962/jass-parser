function YDWEUnitHasItemOfTypeBJNull takes unit whichUnit, integer itemId returns boolean
    local integer index = 0
	if itemId != 0 then
		loop
			if GetItemTypeId(UnitItemInSlot(whichUnit, index)) == itemId then
				return true
			endif
			set index = index + 1
			exitwhen index >= bj_MAX_INVENTORY
		endloop
	endif
    return false
endfunction
