    private function Convert takes integer value, integer length returns string
        local string Char = I2S(value)
        local integer slen = StringLength(Char)
            
        loop
            exitwhen slen >= length
            set Char = "0" + Char
            set slen = slen + 1
        endloop
        
        return Char
    endfunction
   
   
